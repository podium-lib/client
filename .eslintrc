{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "rules": {
      "lines-between-class-members": [0],
      "class-methods-use-this": [0],
      "strict": [0, "global"]
  }
}
